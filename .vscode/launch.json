{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "dt help",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/cli/dist/index.js",
            "args": [
                "-h",
            ],
            "outputCapture": "std",
            "env": {
                "LOG_LEVEL": "verbose",
            },
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "dta srg evaluate",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/cli/dist/index.js",
            "args": [
                "srg",
                "evaluate"
            ],
            "outputCapture": "std",
            "env": {
                "LOG_LEVEL": "verbose",
                "LOG_FILE": "true",
                "SRG_EVALUATION_STAGE": "dev",
                "SRG_EVALUATION_SERVICE": "demo-cli-service",
                "SRG_APP_VERSION": "v1.0.0",
                "SRG_EVALUATION_PROVIDER": "local-vscode",
                "SRG_EVALUATION_BUILD_ID": "1234567890",
            },
            "envFile": "${workspaceFolder}/cli/.env",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "dta event deploy",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/cli/dist/index.js",
            "args": [
                "event",
                "deploy"
            ],
            "outputCapture": "std",
            "env": {
                "LOG_LEVEL": "verbose",
                "LOG_FILE": "true",
                "EVENT_ENTITY_SELECTOR": "type(PROCESS_GROUP_INSTANCE),tag(app:simplenodeservice)",
            },
            "envFile": "${workspaceFolder}/cli/.env",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}